{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/style.css",
      "dest": "/public/style.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/login.js",
      "dest": "/public/login.js"
    },
    {
      "src": "/(login|Index|paso1|paso2|paso3|about|config|help)\\.html",
      "dest": "/views/$1.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ],
  "outputDirectory": "."
}
